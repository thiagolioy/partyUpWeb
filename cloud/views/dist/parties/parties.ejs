<% var stylesheet = "admin" %><% var title = "Admin Parties" %><% include ../_header %><div class="row"><div class="large-12 medium-12 small-12 columns"><table class="responsive"><tr><th>Nome</th><th>Lugar</th><th>Data</th><th>Contato Lista</th><th>Remover</th></tr><% parties.forEach(function(party) { %><tr><td><a href="http://partyup.parseapp.com/parties/<%=party.id%>"><%= party.get('name') %></a></td><td><%= party.get('place').get('name') %></td><td><%= moment(party.get('date')).format('DD/MM/YYYY') %></td><% if(party.get('sendNamesType') == 'facebook') {%><td><a target="_blank" href="https://www.facebook.com/events/<%=party.get('sendNamesTo')%>/?ref_dashboard_filter=upcoming">FacebookEvent</a></td><% }else if(party.get('sendNamesType') == 'none') {%><td>Sem lista</td><% }else {%><td><%=party.get('sendNamesTo')%></td><% }%><td><a rel="nofollow" href="http://partyup.parseapp.com/parties/delete/<%=party.id%>">Remover</a></td></tr><% }) %></table><div class="pagination-centered"><ul class="pagination"><% partiesCount.forEach(function(pageNumber){ %><%if(pageNumber==currentPage){%><li class="current"><a href="http://partyup.parseapp.com/parties?page=<%=pageNumber%>"><%=pageNumber +1%></a></li><%} else {%><li><a href="http://partyup.parseapp.com/parties?page=<%=pageNumber%>"><%=pageNumber + 1%></a></li><%}%><% }) %></ul></div></div></div><% include ../_footer %>